<html>
  <head>
    <title>Members Only</title>
    <link rel="stylesheet" href="./styles.css">
  </head>
  <body>
    <%- include('header') %>
    <main>
      <ul class="messageBoard">
        <% messages.forEach( (message) => { %>
          <!-- <&- include('message, {message: message}') -->
           <li class="message">
            <p><%= message.user_name %></p>
            <p><%= message.message_text %></p>
           </li>
        <% }); %>
      </ul>
    </main>    
    <%- include('footer') %>
  </body>
</html>